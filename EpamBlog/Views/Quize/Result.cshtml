@model EpamBlog.ViewModels.Quiz

@{
    ViewBag.Title = "QuizeResult";
}

<div class="row no-margin-bot">
    <div class="col s7 push-s1">
        <div id="main-body" class="card">
            <div class="row"></div>
            <p class="flow-text welcome-text centralized">Thank you!</p>
            <p class="flow-text sub-welcome-text centralized">The information you're entered is below</p>
            <hr class="colored-hr" />
            <div class="row">
                <div class="col s6 push-s3">
                    <table class="bordered">
                        <tbody>
                            <tr>
                                <td>Name</td>
                                <td>@Model.Name</td>
                            </tr>
                            <tr>
                                <td>Surname</td>
                                <td>@Model.Surname</td>
                            </tr>
                            <tr>
                                <td>Sex</td>
                                <td>@Model.Sex</td>
                            </tr>
                            <tr>
                                <td>Date</td>
                                <td>@Model.DateBirt</td>
                            </tr>
                            <tr>
                                <td>E-mail</td>
                                <td>@Model.Email</td>
                            </tr>
                            <tr>
                                <td>Interest</td>
                                <td>@Model.Position</td>
                            </tr>
                            <tr>
                                <td>About</td>
                                <td>@Model.About</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col s3 push-s1">
        <div class="row">
            <div id="right-sidebar" class="card centralized">
                <div class="row"></div>
                <p class="card-title flow-text">Menu</p>
                <div class="collection with-header">
                    @Html.RouteLink("Home", new { controller = "Home", action = "Index" }, new { @class = "collection-item" })
                    @Html.RouteLink("Reviews", new { controller = "Reviews", action = "Index" }, new { @class = "collection-item" })
                    @Html.RouteLink("Quize", new { controller = "Quize", action = "Index" }, new { @class = "collection-item active" })
                </div>
            </div>
        </div>
    </div>
</div>
